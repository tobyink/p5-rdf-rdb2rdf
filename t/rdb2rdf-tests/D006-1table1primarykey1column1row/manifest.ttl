	test:specificationReference "#rules_table_triples" ;
	test:reviewStatus test:accepted ;
	rdb2rdftest:database <d006> ;
	rdb2rdftest:hasExpectedOutput true ;
	rdb2rdftest:output "directGraph.nt" ;
